<h1>Home page</h1>

<p>User</p>
<% if current_user %>
  <%= link_to "Logout", destroy_user_session_path, data: {
    turbo_method: :delete
  } %>
<% else %>
  <%= link_to 'Login', new_user_session_path %>
  <%= link_to 'Sign up', new_user_registration_path %>
<% end %>
